<!ATTLIST PUB-GRP ID CDATA #IMPLIED>
<!ATTLIST JOURNAL TYPE CDATA #IMPLIED>
<!ATTLIST ADD-JOURNAL TYPE CDATA #IMPLIED>
<!ATTLIST LA-GRP ID CDATA #IMPLIED>
<!ATTLIST REGION ID CDATA #IMPLIED>
<!ATTLIST REGION STATUS CDATA #IMPLIED>
<!ATTLIST FOUND-DATE YEAR CDATA #IMPLIED>
<!ELEMENT UC-GRP (SUB-UC)>
<!ELEMENT SUB-UC (TITLE, SUB-SUB-UC)>

<!ELEMENT SUB-SUB-UC (TITLE, PRIMARY)>
<!ELEMENT PRIMARY (TIBLOCK, BLOCK, ADDONLIST?)>


<!ELEMENT CONTACT (#PCDATA | EMAIL)*>
<!ELEMENT CITY (#PCDATA)>
<!ELEMENT POSTCODE (#PCDATA)>
<!ELEMENT FOUNDED (FOUND-PTEXT)>
<!ELEMENT FOUND-PTEXT (#PCDATA |FOUND-DATE)*>
<!ELEMENT FOUND-DATE EMPTY>
<!ELEMENT CONTROL (#PCDATA)>
<!ELEMENT ACAD-YEAR (FROM-MONTH, TO-MONTH)>
<!ELEMENT FROM-MONTH EMPTY>
<!ATTLIST FROM-MONTH MONTH CDATA #REQUIRED>
<!ELEMENT TO-MONTH EMPTY>
<!ATTLIST TO-MONTH MONTH CDATA #REQUIRED>
<!ELEMENT GEN-INFO (#PCDATA)>
<!ATTLIST GEN-INFO AFTER CDATA #REQUIRED>
<!ATTLIST GEN-INFO BEFORE CDATA #REQUIRED>
<!ATTLIST GEN-INFO HANG CDATA #REQUIRED>

<!ELEMENT NAME (ACAD-TITLE?, VERBATIM-NAME?, FORENAME?, SURNAME?)>
<!ELEMENT ACAD-TITLE (#PCDATA)>
<!ELEMENT VERBATIM-NAME (#PCDATA)>
<!ELEMENT LIB-VOL (LIB-PTEXT)>
