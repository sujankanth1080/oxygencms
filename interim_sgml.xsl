    <xsl:variable name="quote" select="substring($restq,1,1)"/>
    <xsl:variable name="rest"
                  select="substring-before(substring($restq,2),$quote)"/>
    <xsl:if test="$rest!=''">
      <xsl:value-of select="$fontattr"/>
      <xsl:text>: </xsl:text>
      <xsl:value-of select="$rest"/>
      <xsl:text>; </xsl:text>
    </xsl:if>
  </xsl:if>
</xsl:template>

